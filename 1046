import java.util.Scanner;
import java.io.*;
public class Main{
	public static void main(String[] args){
	    PrintStream saida=System.out;
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt(),b = sc.nextInt(),x;
        if(a>=b){
            x = (24-a)+b;
            saida.printf("O JOGO DUROU %d HORA(S)\n",x);
        }else{
            x = b-a;
            saida.printf("O JOGO DUROU %d HORA(S)\n",x);
        }
	}
}
