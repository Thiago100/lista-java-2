import java.util.Scanner;
import java.io.*;
public class Main {
    public static void main(String[] args) {
		PrintStream sa = System.out;
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt(),b = sc.nextInt();
		if(a>b && b<=96 && b>=3) sa.println("minguante");
		else{
			if(b<=2) sa.println("nova");
			else{
				if(b<=96) sa.println("crescente");
				else sa.println("cheia");
			}
			
		}
		
   }
}
